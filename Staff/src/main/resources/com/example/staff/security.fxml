<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="520.0" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="com.example.staff.SecurityController">
    <children>
        <AnchorPane prefHeight="67.0" prefWidth="600.0" style="-fx-background-color: purple;">
            <children>
                <Label alignment="CENTER" layoutX="129.0" layoutY="22.0" prefHeight="17.0" prefWidth="341.0" text="Security Question" textFill="#edeaea">
                    <font>
                        <Font name="Verdana Bold" size="18.0" />
                    </font>
                </Label>
            </children>
        </AnchorPane>
        <Label layoutX="55.0" layoutY="104.0" prefHeight="17.0" prefWidth="522.0" text="How many years has it been since the company was originally established?">
            <font>
                <Font name="Verdana Italic" size="12.0" />
            </font>
        </Label>
        <Label layoutX="57.0" layoutY="143.0" prefHeight="17.0" prefWidth="87.0" text="Answer:      10">
            <font>
                <Font name="System Bold" size="12.0" />
            </font>
        </Label>
        <Label layoutX="14.0" layoutY="104.0" prefHeight="17.0" prefWidth="31.0" text="Q. 1" />
        <Label layoutX="14.0" layoutY="211.0" prefHeight="17.0" prefWidth="31.0" text="Q. 2" />
        <Label layoutX="12.0" layoutY="293.0" prefHeight="43.0" prefWidth="573.0" text="Enter your combined answers into the box below and hit finish (e.g if your answers were 2 &amp; 3 please enter 23 )" textFill="#13a1a6">
            <font>
                <Font name="Verdana Italic" size="10.0" />
            </font>
        </Label>
        <TextField fx:id="answerTextField" layoutX="26.0" layoutY="335.0" promptText="Answer" />
        <Label layoutX="65.0" layoutY="211.0" prefHeight="17.0" prefWidth="522.0" text="How many times a week do we have in office scrum meetings?">
            <font>
                <Font name="Verdana Italic" size="12.0" />
            </font>
        </Label>
        <Label layoutX="220.0" layoutY="143.0" prefHeight="17.0" prefWidth="87.0" text="Answer:      25">
            <font>
                <Font name="System Bold" size="12.0" />
            </font>
        </Label>
        <Label layoutX="401.0" layoutY="143.0" prefHeight="17.0" prefWidth="79.0" text="Answer:     8">
            <font>
                <Font name="System Bold" size="12.0" />
            </font>
        </Label>
        <Label layoutX="65.0" layoutY="265.0" prefHeight="17.0" prefWidth="93.0" text="Answer:     0">
            <font>
                <Font name="System Bold" size="12.0" />
            </font>
        </Label>
        <Label layoutX="193.0" layoutY="265.0" prefHeight="17.0" prefWidth="93.0" text="Answer:     2">
            <font>
                <Font name="System Bold" size="12.0" />
            </font>
        </Label>
        <Label layoutX="326.0" layoutY="265.0" prefHeight="17.0" prefWidth="93.0" text="Answer:     3">
            <font>
                <Font name="System Bold" size="12.0" />
            </font>
        </Label>
        <Label layoutX="441.0" layoutY="265.0" prefHeight="17.0" prefWidth="93.0" text="Answer:     1">
            <font>
                <Font name="System Bold" size="12.0" />
            </font>
        </Label>
        <Button fx:id="finishButton" layoutX="401.0" layoutY="335.0" mnemonicParsing="false" onAction="#finish" prefHeight="25.0" prefWidth="93.0" text="Finish" />
        <TextField fx:id="nameTextField" layoutX="225.0" layoutY="335.0" promptText="Username" />
      <Label fx:id="usernameLabel" layoutX="80.0" layoutY="365.0" prefHeight="17.0" prefWidth="189.0" textFill="#f81616" />
    </children>
</AnchorPane>
